{
  "name": "claude-flow-the-ascension",
  "version": "1.0.0",
  "description": "Interactive game for mastering Claude Flow commands and swarm orchestration",
  "main": "src/index.tsx",
  "scripts": {
    "dev": "vite",
    "start": "./scripts/start-with-update.sh",
    "start:dev": "./scripts/start-game.sh",
    "start:auto": "npx --yes claude-flow@alpha --version && vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "lint": "eslint src --ext ts,tsx",
    "typecheck": "tsc --noEmit",
    "update:claude-flow": "npm cache clean --force && npx --yes claude-flow@alpha --version",
    "update:force": "npm cache clean --force && rm -rf ~/.npm/_npx && npx --yes --ignore-existing claude-flow@alpha --version",
    "predev": "npm run update:claude-flow",
    "server:update": "node server/updateServer.js",
    "dev:with-update": "npm run update:force && concurrently \"npm run server:update\" \"npm run dev\""
  },
  "dependencies": {
    "@react-three/drei": "^9.92.7",
    "@react-three/fiber": "^8.15.12",
    "canvas-confetti": "^1.9.2",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-confetti": "^6.1.0",
    "react-dom": "^18.2.0",
    "react-particles": "^2.12.2",
    "react-spring": "^9.7.3",
    "three": "^0.158.0",
    "tsparticles-slim": "^2.12.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@anthropic-ai/claude-code": "^1.0.72",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/three": "^0.158.3",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "claude-flow": "^2.0.0-alpha.88",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vitest": "^1.0.4"
  }
}
